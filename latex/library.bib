address = {Surabaya},
author = {FATHURRAHMAN, LUTHFI},
keywords = {ANFIS.,Accelerometer,Gyroscope,Sistem pendeteksi kondisi terjatuh},
publisher = {Institut Teknologi Sepuluh Nopember},
title = {{SISTEM PENDETEKSI KONDISI TERJATUH BERBASIS ACCELEROMETER DAN GYROSCOPE MENGGUNAKAN ADAPTIVE NEURO FUZZY INFERENCE SYSTEM}},
