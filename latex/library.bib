issn = {1062922X},
journal = {2014 IEEE International Conference on Systems, Man, and Cybernetics (SMC)},
keywords = {Accelerometers,Accidents,Accuracy,Firing,Legged locomotion,SMA method,SVM,Senior citizens,Support vector machines,accelerometer,accident prevention,activity types,elderly activity pattern,elderly activity type,elderly physical health,emotional trauma,fall accident,fall detection,fall-related research,fuzzy set theory,geriatrics,health care,injuries,interval type-2 fuzzy model,medical diagnostic computing,multilayer detection system,signal detection,signal magnitude area,signal vector magnitude,smart phones},
